	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"@scarf/scarf",
			"@swc/core",
			"@tailwindcss/oxide",
			"@tree-sitter-grammars/tree-sitter-yaml",
			"core-js-pure",
			"esbuild",
			"tree-sitter",
			"tree-sitter-json"
		]
	},
	"scarfSettings": {
		"enabled": false
	}
