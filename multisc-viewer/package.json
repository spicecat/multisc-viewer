		"lodash.debounce": "^4.0.8",
		"lru-cache": "^11.2.2",
		"openapi-fetch": "^0.15.0",
		"swagger-ui-dist": "^5.29.5"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.38.0",
		"@skeletonlabs/skeleton": "^4.1.3",
		"@skeletonlabs/skeleton-svelte": "^4.1.3",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.47.3",
