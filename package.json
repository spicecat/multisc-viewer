  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "concurrently \"npm:dev:multisc-viewer\" \"npm:dev:multisc-server\"",
    "dev:server": "pnpm --filter multisc-server start:dev",
    "dev:viewer": "pnpm --filter multisc-viewer dev",
    "build": "pnpm --filter server build && pnpm --filter multisc-viewer build"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
