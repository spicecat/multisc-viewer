    "@swc/core": "^1.12.7",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/lodash.debounce": "^4.0.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.15.34",
    "concurrently": "^8.2.2",
    "eslint": "^9.30.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  }
}
