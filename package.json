{
	"name": "multisc-viewer",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"pm2:dev": "pm2 start ecosystem.config.js --env development",
		"pm2:prod": "pm2 start ecosystem.config.js --env production",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview"
	},
	"dependencies": {
		"axios": "^1.11.0",
		"fuse.js": "^7.1.0",
		"html2canvas": "^1.4.1",
		"lodash.debounce": "^4.0.8",
		"node-cache": "^5.1.2"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.32.0",
		"@lucide/svelte": "^0.535.0",
		"@skeletonlabs/skeleton": "^3.1.7",
		"@skeletonlabs/skeleton-svelte": "^1.3.1",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.27.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/vite": "^4.1.11",
		"@types/lodash.debounce": "^4.0.9",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"sass-embedded": "^1.89.2",
		"svelte": "^5.37.2",
		"svelte-check": "^4.3.0",
		"svelte-dnd-action": "^0.9.64",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.38.0",
		"vite": "^7.0.6"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
